                                        MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                            UseFastattack = true
                                        else
                                            UseFastattack = false
                                        end
                                    until not SaveSettings["Farm"]['Auto Haki Rainbow'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                    UseFastattack = false
                                    IsnetworkOwnerMon = nil
                                    Positionmon = nil
                                end
                            else
                                MAZI_FUNCTION:TP(CFrame.new(2879, 433, -7090))
                            end	
                        elseif string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Captain Elephant") then
                            if game.Workspace.Enemies:FindFirstChild("Captain Elephant [Lv. 1875] [Boss]") then
                                if v.Name == "Captain Elephant [Lv. 1875] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    repeat wait()
                                        Buso()
                                        IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                        MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                            UseFastattack = true
                                        else
                                            UseFastattack = false
                                        end
                                    until not SaveSettings["Farm"]['Auto Haki Rainbow'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                    UseFastattack = false
                                    IsnetworkOwnerMon = nil
                                    Positionmon = nil
                                end
                            else
                                MAZI_FUNCTION:TP(CFrame.new(-13348, 406, -8574))
                            end	
                        elseif string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Beautiful Pirate") then
                            if game.Workspace.Enemies:FindFirstChild("Beautiful Pirate [Lv. 1950] [Boss]") then
                                if v.Name == "Beautiful Pirate [Lv. 1950] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    repeat wait()
                                        Buso()
                                        IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                        MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                            UseFastattack = true
                                        else
                                            UseFastattack = false
                                        end
                                    until not SaveSettings["Farm"]['Auto Haki Rainbow'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                    UseFastattack = false
                                    IsnetworkOwnerMon = nil
                                    Positionmon = nil
                                end
                            else
                                if (CFrame.new(5310, 23, 131).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 330 then
                                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",CFrame.new(5314, 23, -125).Position)
                                end
                            end	
                        end
                    else
                        local string_1 = "HornedMan";
                        local string_2 = "Bet";
                        local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                        Target:InvokeServer(string_1, string_2);
                        local string_1 = "HornedMan";
                        local string_2 = "Bet";
                        local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                        Target:InvokeServer(string_1, string_2);
                    end
                else
                    local string_1 = "HornedMan";
                    local string_2 = "Bet";
                    local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                    Target:InvokeServer(string_1, string_2);
                    local string_1 = "HornedMan";
                    local string_2 = "Bet";
                    local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                    Target:InvokeServer(string_1, string_2);
                end
            end
        end
    end
end
function AutoFarmProtal()
    while wait() do
        if SaveSettings["Farm"]['Auto Portal'] or SaveSettings["Farm"]['Auto Farm Cake Prince'] then
            if MAZI_FUNCTION:CheckInIsland(CFrame.new(-1954, 215, -12488)) == true or game:GetService("Workspace").Map:FindFirstChild('MirrorDimension') then
                if game:GetService("Workspace").Map:FindFirstChild('MirrorDimension') and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFrame.new(-1991, 4533, -14973).Position).magnitude > 1000 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.CakeLoaf.BigMirror.Main.CFrame
                else
                    for i,v in pairs(GetAllMob()) do
                        if SaveSettings["Farm"]['Auto Portal'] then
                            if v.Name == "Cookie Crafter [Lv. 2200]" or v.Name == "Cake Guard [Lv. 2225]" or v.Name == "Baking Staff [Lv. 2250]" or v.Name == "Head Baker [Lv. 2275]" then -- v.Name == "Dough King [Lv. 2300] [Raid Boss]" then
                                if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    repeat wait()
                                        Buso()
                                        NameMon = v.Name
                                        IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                        Positionmon = v.HumanoidRootPart.CFrame
                                        MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,30,30))
                                        if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 60 then
                                            usebringmob = true
                                            UseFastattack = true
                                        else
                                            UseFastattack = false
                                            usebringmob = false
                                        end 
                                    until not SaveSettings["Farm"]['Auto Portal'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                    UseFastattack = false
                                    IsnetworkOwnerMon = nil
                                    Positionmon = nil
                                end
                            end
                        end
                        if SaveSettings["Farm"]['Auto Farm Cake Prince'] then
                            if game:GetService('Players').LocalPlayer.Backpack:FindFirstChild("God's Chalice") or game:GetService('Players').LocalPlayer.Character:FindFirstChild("God's Chalice") then
                                game.ReplicatedStorage.Remotes.CommF_:InvokeServer("SweetChaliceNpc") 
                            elseif game:GetService('Players').LocalPlayer.Backpack:FindFirstChild("Sweet Chalice") or game:GetService('Players').LocalPlayer.Character:FindFirstChild("Sweet Chalice") then
                                EquipWeapon('Sweet Chalice')
                                wait()
                                game.ReplicatedStorage.Remotes.CommF_:InvokeServer("CakePrinceSpawner")
                            else
                                game.ReplicatedStorage.Remotes.CommF_:InvokeServer("CakePrinceSpawner")
                            end
                            if game:GetService("Workspace").Map:FindFirstChild('MirrorDimension') then
                                if game.Workspace.Enemies:FindFirstChild('Cake Prince [Lv. 2300] [Raid Boss]') or game.Workspace.Enemies:FindFirstChild('Dough King [Lv. 2300] [Raid Boss]') then
                                    if v.Name == "Cake Prince [Lv. 2300] [Raid Boss]" or v.Name == "Dough King [Lv. 2300] [Raid Boss]" then
                                        repeat wait()
                                            Buso()
                                            NameMon = v.Name
                                            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                                IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                                Positionmon = v.HumanoidRootPart.CFrame
                                                if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 60 then
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,60,40)
                                                else
                                                    MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,50,25))
                                                end
                                                if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 60 then
                                                    usebringmob = true
                                                    UseFastattack = true
                                                else
                                                    UseFastattack = false
                                                    usebringmob = false
                                                end
                                            end 
                                        until not SaveSettings["Farm"]['Auto Farm Cake Prince'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or game.Players.LocalPlayer.Character.Humanoid.Health <= 0
                                        UseFastattack = false
                                        IsnetworkOwnerMon = nil
                                        Positionmon = nil
                                    end
                                else
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2151.82153, 149.315704, -12404.9053)
                                    wait(2)
                                end
                            else
                                if v.Name == "Cookie Crafter [Lv. 2200]" or v.Name == "Cake Guard [Lv. 2225]" or v.Name == "Baking Staff [Lv. 2250]" or v.Name == "Head Baker [Lv. 2275]" then -- v.Name == "Dough King [Lv. 2300] [Raid Boss]" then
                                    if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                        repeat wait()
                                            Buso()
                                            NameMon = v.Name
                                            IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                            Positionmon = v.HumanoidRootPart.CFrame
                                            MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,30,30))
                                            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 60 then
                                                usebringmob = true
                                                UseFastattack = true
                                            else
                                                UseFastattack = false
                                                usebringmob = false
                                            end 
                                        until not SaveSettings["Farm"]['Auto Farm Cake Prince'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                        UseFastattack = false
                                        IsnetworkOwnerMon = nil
                                        Positionmon = nil
                                    end
                                end
                            end
                        end
                    end
                end
            else
                if game:GetService("Workspace").Map:FindFirstChild('MirrorDimension') then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2151.82153, 149.315704, -12404.9053)
                else
                    MAZI_FUNCTION:TP(CFrame.new(-1954, 215, -12488))
                end
            end
        end
    end
end
function AutoFarmSoulReaper()
    while wait() do
        if SaveSettings["Farm"]['Auto Farm Soul Reaper'] then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFrame.new(-9521, 316, 6652).Position).magnitude <= 1000 then
                if game.Players.LocalPlayer.Backpack:FindFirstChild("Hallow Essence") or game.Players.LocalPlayer.Character:FindFirstChild("Hallow Essence") then
                    MAZI_FUNCTION:TP(CFrame.new(-8933, 147, 6064))
                elseif game:GetService("Workspace").Enemies:FindFirstChild("Soul Reaper [Lv. 2100] [Raid Boss]") or game.ReplicatedStorage:FindFirstChild("Soul Reaper [Lv. 2100] [Raid Boss]") then
                    if game:GetService("Workspace").Enemies:FindFirstChild("Soul Reaper [Lv. 2100] [Raid Boss]") then
                        for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                            if SaveSettings["Farm"]['Auto Farm Soul Reaper'] and v.Name == "Soul Reaper [Lv. 2100] [Raid Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                repeat wait()
                                    Buso()
                                    IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                    MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                    if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                        UseFastattack = true
                                    else
                                        UseFastattack = false
                                    end
                                until not SaveSettings["Farm"]['Auto Farm Soul Reaper'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                UseFastattack = false
                                IsnetworkOwnerMon = nil
                                Positionmon = nil
                            end
                        end
                    else
                        MAZI_FUNCTION:TP(CFrame.new(-9521, 316, 6652))
                    end 
                else
                    local string_1 = "Bones";
                    local string_2 = "Buy";
                    local number_1 = 1;
                    local number_2 = 1;
                    local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                    Target:InvokeServer(string_1, string_2, number_1, number_2);
                end
            else
                if game.Players.LocalPlayer.Backpack:FindFirstChild("Hallow Essence") then
                    MAZI_FUNCTION:TP(CFrame.new(-8933, 147, 6064))
                else
                    MAZI_FUNCTION:TP(CFrame.new(-9521, 316, 6652))
                end
            end
        end
    end
end
function AutoRandomBone()
    while wait() do
        if SaveSettings["Farm"]['Auto Random Bone'] then
            local string_1 = "Bones";
            local string_2 = "Buy";
            local number_1 = 1;
            local number_2 = 1;
            local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
            Target:InvokeServer(string_1, string_2, number_1, number_2);
        end
    end
end
function AutoFarmMaterial()
    while wait() do 
        if SaveSettings["Farm"]['Auto Farm Materials'] and SaveSettings["Farm"]['Materials'] ~= 'None' then
            if game.PlaceId == DataMaterials[SaveSettings["Farm"]['Materials']]['Sea'] then
                local TargetIL , err = MAZI_FUNCTION:INIS(DataMaterials[SaveSettings["Farm"]['Materials']]['Island'])
                local INIS = MAZI_FUNCTION:INIS(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
                if game:GetService("Workspace").Enemies:FindFirstChild(DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][1]) or
                    (DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][2] ~= nil and game:GetService("Workspace").Enemies:FindFirstChild(DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][2])) or 
                    (DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][3] ~= nil and game:GetService("Workspace").Enemies:FindFirstChild(DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][3])) or 
                    (DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][4] ~= nil and game:GetService("Workspace").Enemies:FindFirstChild(DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][4]))
                then
                    for i,v in pairs(GetAllMob()) do
                        if v.Name == DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][1] or  
                            (DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][2] ~= nil and v.Name == DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][2]) or 
                            (DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][3] ~= nil and v.Name == DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][3]) or 
                            (DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][4] ~= nil and v.Name == DataMaterials[SaveSettings["Farm"]['Materials']]['NPC'][4])
                        then
                            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and SaveSettings["Farm"]['Auto Farm Materials'] then
                                repeat wait()
                                    Buso()
                                    NameMon = v.Name
                                    IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                    Positionmon = v.HumanoidRootPart.CFrame
                                    MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                    if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                        usebringmob = true
                                        UseFastattack = true
                                    else
                                        UseFastattack = false
                                        usebringmob = false
                                    end
                                until not SaveSettings["Farm"]['Auto Farm Materials'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                UseFastattack = false
                                IsnetworkOwnerMon = nil
                                Positionmon = nil
                            end
                        end
                    end
                else
                    MAZI_FUNCTION:TP(DataMaterials[SaveSettings["Farm"]['Materials']]['Island'])
                end
            else
                if DataMaterials[SaveSettings["Farm"]['Materials']]['Sea'] == 2753915549 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer('TravelMain')
                elseif DataMaterials[SaveSettings["Farm"]['Materials']]['Sea'] == 4442272183 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer('TravelDressrosa')
                elseif  DataMaterials[SaveSettings["Farm"]['Materials']]['Sea'] == 7449423635 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer('TravelZou')
                end
            end
        end
    end
end
function AutoBartiloQuest()
    while wait() do 
        if SaveSettings["Farm"]['Auto Bartilo Quest'] then
            local BartiloQuestProgress = game.ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("BartiloQuestProgress");
            local KilledBandits = BartiloQuestProgress.KilledBandits;
            local KilledSpring = BartiloQuestProgress.KilledSpring;
            local DidPlates = BartiloQuestProgress.DidPlates;
            if KilledBandits then
                if KilledSpring then
                    if not DidPlates then
                        game.ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("BartiloQuestProgress", "DidPlates")
                    end
                else
                    if game.Workspace.Enemies:FindFirstChild("Jeremy [Lv. 850] [Boss]") then
                        for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                            if v.Name == "Jeremy [Lv. 850] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and SaveSettings["Farm"]['Auto Bartilo Quest'] then
                                repeat wait()
                                    Buso()
                                    NameMon = v.Name
                                    IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                    Positionmon = v.HumanoidRootPart.CFrame
                                    MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,20,0))
                                    if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                        usebringmob = true
                                        UseFastattack = true
                                    else
                                        UseFastattack = false
                                        usebringmob = false
                                    end
                                until not SaveSettings["Farm"]['Auto Bartilo Quest']  or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                UseFastattack = false
                                IsnetworkOwnerMon = nil
                                Positionmon = nil
                            end
                        end
                    else
                        MAZI_FUNCTION:TP(CFrame.new(2099.88159, 448.931, 648.997375))
                    end
                end
            else
                if not string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "/50") then 
                    local args = { [1] = "StartQuest", [2] = "BartiloQuest", [3] = 1}
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                else
                    local Q = MAZI_FUNCTION:GetQuest(776)
                    if game.Workspace.Enemies:FindFirstChild("Swan Pirate [Lv. 775]") then
                        for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                            if v.Name == "Swan Pirate [Lv. 775]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and  SaveSettings["Farm"]['Auto Bartilo Quest'] then
                                repeat wait()
                                    Buso()
                                    NameMon = v.Name
                                    IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                    Positionmon = v.HumanoidRootPart.CFrame
                                    MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,20,0))
                                    if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                        usebringmob = true
                                        UseFastattack = true
                                    else
                                        UseFastattack = false
                                        usebringmob = false
                                    end
                                until not SaveSettings["Farm"]['Auto Bartilo Quest'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                UseFastattack = false
                                IsnetworkOwnerMon = nil
                                Positionmon = nil
                            end
                        end
                    else
                        MAZI_FUNCTION:TP(MAZI_FUNCTION:GetRegions(Q.Quest.Mon).CFrame)
                    end
                end
            end
        end
    end
end
function AutoSoulGuitar()
    while wait() do
        if SaveSettings["Farm"]['Auto Soul Guitar'] then
            local GuitarPuzzleProgressCheck = game.ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("GuitarPuzzleProgress", "Check");
            if GuitarPuzzleProgressCheck ~= nil then
                if game:GetService("Workspace").Map["Haunted Castle"].SwampWater.BrickColor == BrickColor.new("Maroon") and GuitarPuzzleProgressCheck.Swamp == false then
                    if game:GetService("Workspace").Enemies:FindFirstChild('Living Zombie [Lv. 2000]') then
                        spawn(function()
                            while wait() do
                                if (SaveSettings["Farm"]['Auto Soul Guitar'] or SaveSettings["Farm"]['Find Full Moon [ HOP ]']) and GuitarPuzzleProgressCheck.Swamp == false then
                                    pcall(function()
                                        for i,v in next ,game:GetService("CollectionService"):GetTagged("ActiveRig") do
                                            if v.Parent then
                                                if isnetworkowner(v.HumanoidRootPart) and v.Name == "Living Zombie [Lv. 2000]" then
                                                    v.HumanoidRootPart.CFrame = CFrame.new(-10158, 138, 5960)
                                                    v.HumanoidRootPart.CanCollide = false
                                                    v.Humanoid:ChangeState(11)
                                                end
                                            end
                                        end
                                    end)
                                end
                            end
                        end)
                        if SaveSettings["Farm"]['Auto Soul Guitar'] then
                            for i,v in next,game:GetService("CollectionService"):GetTagged("ActiveRig") do
                                if SaveSettings["Farm"]['Auto Soul Guitar'] and v.Name == "Living Zombie [Lv. 2000]" and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                                    repeat wait() 
                                        NameMon = v.Name
                                        IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                        Positionmon = v.HumanoidRootPart.CFrame
                                        MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 300 then
                                            usebringmob = true
                                            UseFastattack = true
                                        else
                                            UseFastattack = false
                                            usebringmob = false
                                        end
                                    until not SaveSettings["Farm"]['Auto Soul Guitar'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or GuitarPuzzleProgressCheck.Swamp == true
                                    UseFastattack = false
                                    IsnetworkOwnerMon = nil
                                    Positionmon = nil
                                end
                            end
                        end
                    else
                        MAZI_FUNCTION:TP(CFrame.new(-10315.4531, 216.697418, 5967.41064))
                    end
                else
                    if GuitarPuzzleProgressCheck.Gravestones == false then
                        game.ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("GuitarPuzzleProgress", "Gravestones")
                    else
                        if GuitarPuzzleProgressCheck.Ghost == false then
                            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("GuitarPuzzleProgress", "Ghost")
                        else
                            if GuitarPuzzleProgressCheck.Trophies == false then
                                game.ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("GuitarPuzzleProgress", "Trophies");
                            else
                                if GuitarPuzzleProgressCheck.Pipes == false then
                                    game.ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("GuitarPuzzleProgress", "Pipes")
                                end
                            end
                        end
                    end
                end
            else
                if game:GetService("Lighting"):GetAttribute("MoonPhase") == 5 then
                    MAZI_FUNCTION:TP(CFrame.new(-8653, 143, 6173))
                    game.ReplicatedStorage.Remotes.CommF_:InvokeServer("gravestoneEvent", 2)
                    game.ReplicatedStorage.Remotes.CommF_:InvokeServer("gravestoneEvent", 2, true)
                else
                    if SaveSettings["Farm"]['Find Full Moon [ HOP ]'] then
                        HopServer()
                    end
                end
            end
        end
    end
end
-- [[ Raids ]] --
function NextIsland()
    while wait() do
        if SaveSettings["Raids"]['Auto Next Island'] then
            if game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                if game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5") then
                    game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame.z)
                    if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                        MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame)
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") then
                    game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame.z)
                    if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                        MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame)
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") then
                    game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame.z)
            
                    if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                        MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame)
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") then
                    game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame.z)
                    
                    if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                        MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame)
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                    game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame.z)
                    
                    if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                        MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame)
                    end
                end
            end
        end
    end
end
function RaidsA()
    while wait() do
        if SaveSettings["Raids"]['Kill Aura'] then
            for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                if SaveSettings["Raids"]['Kill Aura'] and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and (v.HumanoidRootPart.Position-game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 1000 then
                    pcall(function()
                        repeat wait()
                            isnetworkowner(v.HumanoidRootPart)
                            v.Humanoid.Health = 0
                        until not SaveSettings["Raids"]['Kill Aura'] or v.Humanoid.Health <= 0 or not v.Parent
                    end)
                end
            end
        end
    end
end
function AutoRaids()
    while wait() do
        if SaveSettings["Raids"]['Auto Raids'] then
            if game.Players.LocalPlayer.Backpack:FindFirstChild('Special Microchip') or game.Players.LocalPlayer.Character:FindFirstChild('Special Microchip') then
                fireclickdetector(Workspace.Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
                game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible = true
                wait(5)
            elseif game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == true then
                for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                    if SaveSettings["Raids"]['Auto Raids'] and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and (v.HumanoidRootPart.Position-game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 1000 then
                        pcall(function()
                            repeat wait()
                                isnetworkowner(v.HumanoidRootPart)
                                v.Humanoid.Health = 0
                                if game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                                    if game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame.z)
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame.z)
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame.z)
                                
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame.z)
                                        
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame.z)
                                        
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame)
                                        end
                                    end
                                end
                            until not SaveSettings["Raids"]['Auto Raids'] or v.Humanoid.Health <= 0 or not v.Parent
                        end)
                    end
                end
            else
                game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("RaidsNpc", "Select", SaveSettings["Raids"]['Select Raids'])
            end
        end
    end
end
function AutoRaidsHop()
    while wait() do
        if SaveSettings["Raids"]['Auto Raids [ Hop ]'] then
            if game.Players.LocalPlayer.Backpack:FindFirstChild('Special Microchip') or game.Players.LocalPlayer.Character:FindFirstChild('Special Microchip') then
                fireclickdetector(Workspace.Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
                game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible = true
                wait(5)
            elseif game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == true then
                for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                    if SaveSettings["Raids"]['Auto Raids [ Hop ]'] and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and (v.HumanoidRootPart.Position-game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 1000 then
                        pcall(function()
                            repeat wait()
                                isnetworkowner(v.HumanoidRootPart)
                                v.Humanoid.Health = 0
                                if game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") or game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                                    if game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame.z)
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame.z)
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame.z)
                                
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame.z)
                                        
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame.z)
                                        
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame)
                                        end
                                    end
                                end
                            until not SaveSettings["Raids"]['Auto Raids [ Hop ]'] or v.Humanoid.Health <= 0 or not v.Parent
                        end)
                    end
                end
            else
                if SaveSettings["Raids"]['Auto Raids [ Hop ]'] then
                    if game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("RaidsNpc", "Select", SaveSettings["Raids"]['Select Raids']):find("physical fruit to ignore this cooldown.") or 
                        game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("RaidsNpc", "Select", SaveSettings["Raids"]['Select Raids']):find("buy another Special Microchip.]") 
                    then
                        if SaveSettings["Raids"]['Auto Raids Hop Find Fruit'] then
                            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                                if v:IsA("Tool") and string.find(v.Name,"Fruit") then
                                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,v.Handle,0)
                                    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                                        if string.find(v.Name,"Fruit") then
                                            local FruitName = RemoveTextFruit(v.Name)
                                            if v.Name == "Bird: Falcon Fruit" then
                                                NameFruit = "Bird-Bird: Falcon"
                                            elseif v.Name == "Bird: Phoenix Fruit" then
                                                NameFruit = "Bird-Bird: Phoenix"
                                            elseif v.Name == "Human: Buddha Fruit" then
                                                NameFruit = "Human-Human: Buddha"
                                            else
                                                NameFruit = FruitName.."-"..FruitName
                                            end
                        
                                            local string_1 = "getInventoryFruits";
                                            local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                                            for i1,v1 in pairs(Target:InvokeServer(string_1)) do
                                                if v1.Name == NameFruit then
                                                    HaveFruitInStore = true
                                                end
                                            end
                                            if not HaveFruitInStore then
                                                local args = {
                                                    [1] = "StoreFruit",
                                                    [2] = NameFruit,
                                                    [3] = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v.Name)
                                                }
                                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                                            end
                                            HaveFruitInStore = false
                                        end
                                    end
                                end
                            end
                            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                                if string.find(v.Name,"Fruit") then
                                    local FruitName = RemoveTextFruit(v.Name)
                                    if v.Name == "Bird: Falcon Fruit" then
                                        NameFruit = "Bird-Bird: Falcon"
                                    elseif v.Name == "Bird: Phoenix Fruit" then
                                        NameFruit = "Bird-Bird: Phoenix"
                                    elseif v.Name == "Human: Buddha Fruit" then
                                        NameFruit = "Human-Human: Buddha"
                                    else
                                        NameFruit = FruitName.."-"..FruitName
                                    end
                
                                    local string_1 = "getInventoryFruits";
                                    local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                                    for i1,v1 in pairs(Target:InvokeServer(string_1)) do
                                        if v1.Name == NameFruit then
                                            HaveFruitInStore = true
                                        end
                                    end
                                    if not HaveFruitInStore then
                                        local args = {
                                            [1] = "StoreFruit",
                                            [2] = NameFruit,
                                            [3] = game:GetService("Players").LocalPlayer.Character:FindFirstChild(v.Name)
                                        }
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                                    end
                                    HaveFruitInStore = false
                                end
                            end
                            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                                if v:IsA("Model") and string.find(v.Name,"Fruit") then
                                    MAZI_FUNCTION:Tween(v.Handle.CFrame)
                                end
                            end
                        elseif SaveSettings["Raids"]['Auto Raids Use Fruit in Inventory'] then

                        end
                    end
                end
            end
        end
    end
end
-- [[ Bring Mob ]] --
function BringMob()
    while wait() do
        if SaveSettings["Setting Auto Farm"]['Bring Mob'] and usebringmob then
            if SaveSettings["Farm"]['Auto Rengoku'] or SaveSettings["Farm"]['Auto Farm Buddy Swords'] or SaveSettings["Farm"]['Auto Bartilo Quest'] or SaveSettings["Farm"]['Auto Farm Skip'] or SaveSettings["Farm"]['Auto Farm Cake Prince'] or SaveSettings["Farm"]['Auto Portal'] or SaveSettings["Farm"]['Auto Farm'] or SaveSettings["Farm"]['Auto Farm Materials'] then
                pcall(function()
                    for i,v in next ,GetAllMob() do
                        if v.Parent then
                            if NameMon and isnetworkowner(v.HumanoidRootPart) and v.Name == NameMon and Positionmon and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 500 then
                                v.HumanoidRootPart.CFrame = Positionmon
                                v.HumanoidRootPart.CanCollide = false
                                v.Humanoid:ChangeState(11)
                            end
                        end
                    end
                end)
            end
        end
    end
end
-- [[ Fast Attack ]] --
local CombatFramework = require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework)
function CheckToolFromTip()
    local v = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") do
        if v:IsA("Tool") and (v.ToolTip == 'Melee' or v.ToolTip == 'Sword') then
            return true
        else
            return false
        end
    end
end
local function removespaces(str)
    return str:gsub(" ","")
end
local countbypass = 0
function bypassdelay()
    if countbypass <= 11 and CheckToolFromTip() == true then
        if game.Players.LocalPlayer.Character:FindFirstChildOfClass('Tool') then
            -- Sea 1
            if game.Players.LocalPlayer.Character:FindFirstChild('Soul Cane') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","SwordCane")
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Saber') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","ShanksCutlass")
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Pole (1st Form)') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","Pole")
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Dual-Headed Blade') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","Spear")
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Dragon Trident') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","HolyTrident")
            -- Sea 2
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Pole (2nd Form)') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","Pole2")
            -- Sea 3
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Hallow Scythe') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","Scythe")
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Canvander') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","Durandal")
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Cursed Dual Katana') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","DualCursed")
            -- Combaet
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Death Step') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","BlackLeg2")
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Sharkman Karate') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","FishmanKarate2")
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Electric Claw') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","Electro2")
            elseif game.Players.LocalPlayer.Character:FindFirstChild('Saber') then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange","ShanksCutlass")
            else
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange",removespaces(game.Players.LocalPlayer.Character:FindFirstChildOfClass('Tool').Name))
            end
        end
    end 
    countbypass = countbypass + 1
end
spawn(function()
    while wait(1) do
        countbypass = 0
    end
end)
local plr = game.Players.LocalPlayer
local CbFw = debug.getupvalues(require(plr.PlayerScripts.CombatFramework))[2]
function AttackNoCD()
    pcall(function()
        local AC = CbFw.activeController
        for i=1,1 do
            local bladehit = require(game:GetService("ReplicatedStorage").CombatFramework.RigLib).getBladeHits(plr.Character,{plr.Character.HumanoidRootPart},80)
            local cac = {}
            local hash = {}
            for i,v in next,bladehit do 
                if v.Parent:FindFirstChild("HumanoidRootPart") and not hash[v.Parent] then
                    table.insert(cac,v.Parent.HumanoidRootPart)
                    hash[v.Parent] = true
                end
            end
            bladehit = cac
            if #bladehit > 0 and AC.attack then 
                local u8 = debug.getupvalue(AC.attack,5)
                local u9 = debug.getupvalue(AC.attack,6)
                local u7 = debug.getupvalue(AC.attack,4)
                local ColorData = debug.getupvalue(AC.attack,7)
                local u12 = (u8 * 798405 + u7 * 727595) % u9
                local u13 = u7 * 798405 (function() u12 = (u12 * u9 +u13) % 1099511627776  u8 = math.floor(u12/u9) u7 = u12 - u8 * u9 end)()
                ColorData = ColorData + 1 
                debug.setupvalue(AC.attack,4,u7)
                debug.setupvalue(AC.attack,5,u8)
                debug.setupvalue(AC.attack,6,u9)
                debug.setupvalue(AC.attack,7,ColorData)
                if plr.Character:FindFirstChildOfClass("Tool") and AC.blades and AC.blades[1] and countbypass <= 11 and IsnetworkOwnerMon then --and #game:GetService("Workspace").Enemies:GetChildren() >= 3 then
                    for i,v in next,AC.animator.anims.basic do v:Play(0.01,0.01,0.01) end
                    bypassdelay()
                    game:GetService("ReplicatedStorage").RigControllerEvent:FireServer(math.floor(u12/ 1099511627776 * 16777215),ColorData)
                    game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("hit",bladehit,#AC.anims.basic,"")
                end
            end
        end
    end)
end
local countattack = 0
function FastAttack()
    game:GetService("RunService").Stepped:Connect(function()
        if SaveSettings["Setting Auto Farm"]['Fast Attack'] and UseFastattack then
            if SaveSettings["Farm"]['Auto Farm Soul Reaper'] or SaveSettings["Farm"]['Auto Rengoku'] or SaveSettings["Farm"]['Auto Farm Boss Law'] or SaveSettings["Farm"]['Auto Farm Buddy Swords'] or SaveSettings["Farm"]['Auto Training Dummy'] or SaveSettings["Farm"]['Auto Soul Guitar'] or SaveSettings["Farm"]['Auto Farm Skip'] or SaveSettings["Farm"]['Auto Farm Cake Prince'] or SaveSettings["Farm"]['Auto Bartilo Quest'] or SaveSettings["Farm"]['Auto Portal'] or SaveSettings["Farm"]['Auto Elite Hunter Hop'] or SaveSettings["Farm"]['Auto Elite Hunter'] or SaveSettings["Farm"]['Auto Farm'] or SaveSettings["Farm"]['Auto Farm Materials'] then
                pcall(function()
                    local abcCombatFramework = getupvalues(CombatFramework)[2]['activeController']
                    if abcCombatFramework and abcCombatFramework.timeToNextAttack then
                        abcCombatFramework.timeToNextAttack = 0
                        abcCombatFramework.hitboxMagnitude = 80
                        abcCombatFramework.increment = #abcCombatFramework.anims.basic
                        abcCombatFramework:attack(abcCombatFramework)
                    end
                end)
            end
        end
    end)
end
function AttackCD()
    while wait() do
        if SaveSettings["Setting Auto Farm"]['Fast Attack'] and UseFastattack then
            if SaveSettings["Farm"]['Auto Farm Soul Reaper'] or SaveSettings["Farm"]['Auto Rengoku'] or SaveSettings["Farm"]['Auto Farm Boss Law'] or SaveSettings["Farm"]['Auto Farm Buddy Swords'] or SaveSettings["Farm"]['Auto Training Dummy'] or SaveSettings["Farm"]['Auto Soul Guitar'] or SaveSettings["Farm"]['Auto Farm Skip'] or SaveSettings["Farm"]['Auto Farm Cake Prince'] or SaveSettings["Farm"]['Auto Bartilo Quest'] or SaveSettings["Farm"]['Auto Portal'] or SaveSettings["Farm"]['Auto Elite Hunter Hop'] or SaveSettings["Farm"]['Auto Elite Hunter'] or SaveSettings["Farm"]['Auto Farm'] or SaveSettings["Farm"]['Auto Farm Materials'] then
                AttackNoCD() 
            end
        end
    end
end
--[[
Magma Ninja [Lv. 1597]
Lava Pirate [Lv. 1478]
