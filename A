                                end
                            end)
                        end

                        Title.MouseButton1Down:Connect(function()
                            DropdownToggled = not DropdownToggled
                            
                            if DropdownToggled then
                                if table_function.textbox == true then
                                    Find.Visible = false
                                    Title.TextTransparency = 0
