function Load()
    if readfile and writefile and isfile and isfolder then
        if not isfolder("AnonymousHub") then
            makefolder("AnonymousHub")
        end
        if not isfolder("/AnonymousHub/BFsave") then
            makefolder("/AnonymousHub/BFsave")
        end
        if not isfolder("/AnonymousHub/BFsave/data") then
            makefolder("/AnonymousHub/BFsave/data")
        end
        if not isfile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json") then
            repeat wait()
                writefile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json", game:GetService("HttpService"):JSONEncode(SaveSettings))
            until isfile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json")
        else
            local Decode = game:GetService("HttpService"):JSONDecode(readfile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json"))
            for i,v in pairs(Decode) do
                SaveSettings[i] = v
            end
        end
    else
        warn("Failed Load")
        return false
    end
end
function Save()
    if readfile and writefile and isfile then
        if isfile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json") == false then
            Load()
        elseif not isfile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json") then
            repeat wait()
                writefile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json", game:GetService("HttpService"):JSONEncode(SaveSettings))
            until isfile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json")
        else
            local Decode = game:GetService("HttpService"):JSONDecode(readfile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json"))
            local Array = {}
            for i,v in pairs(SaveSettings) do
                Array[i] = v
            end
            writefile("/AnonymousHub/BFsave/" .. tostring(game.Players.LocalPlayer.UserId) .. ".json", game:GetService("HttpService"):JSONEncode(Array))
        end
    else
        warn("Failed Save")
        return false
    end
end
Load()
Save()
