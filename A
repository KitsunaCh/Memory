                        BValue.Name = "BValue"
                        BValue.Parent = ColorPicker
                        BValue.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
                        BValue.Position = UDim2.new(0, 170, 0, 60)
                        BValue.Size = UDim2.new(0, 42, 0, 19)
