                        TitleTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
                        TitleTitle.TextSize = 18.000
                        TitleTitle.TextXAlignment = Enum.TextXAlignment.Left
                     
                        local Title = Instance.new("TextLabel")
                        Title.Name = "Title"
                        Title.Parent = TitleTitle
                        Title.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
                        Title.Position = UDim2.new(-0.0278330017, 0, 0.022457853, 0)
                        Title.Transparency = 1
                        Title.Size = UDim2.new(0, 475, 0, 35)
                        Title.ZIndex = 5
                        Title.Font = Enum.Font.SourceSansBold
                        Title.Text = "          "..table_function.text
                        Title.TextColor3 = Color3.fromRGB(255, 255, 255)
                        Title.TextSize = 18.000
                        Title.TextXAlignment = Enum.TextXAlignment.Left
                        
                        UICorner.Parent = TitleTitle

                        local LabelFunc = {}

                        function LabelFunc:SetText(text)
                            TitleTitle.Text = "     "..text
                        end

                        return LabelFunc
                    elseif mode == "btn" then
                        local ButtonButton = Instance.new("Frame")
                        local Button = Instance.new("TextButton")
                        local touch_app = Instance.new("ImageButton")
                        local ButtonRounded = Instance.new("ImageLabel")
