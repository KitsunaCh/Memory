end
-- [[ Get Data ]] --
function GetAllMob(v)
    if v == 'all' then
        return game:GetService("CollectionService"):GetTagged("ActiveRig")
    else
        return game:GetService("Workspace").Enemies:GetChildren()
    end
end

function MAZI_FUNCTION:GetAllNpcQuest()
    local AllNpcQuest = {}
    local GuideModule = require(game:GetService("ReplicatedStorage").GuideModule)
    for i,v in next,GuideModule.Data.NPCList do
        for i2,v2 in next,v do
            local Levels = v.Levels
            for i=1,#Levels do
                if v.NPCName ~= 'Marine Leader' then
                    AllNpcQuest[Levels[i]] = {
                        NPCName = v.NPCName,
                        NPCPosition = v.Position
                    }
                end
            end
        end
    end
    return AllNpcQuest
end
function MAZI_FUNCTION:GetInfoNPCQuest(Level)
    local Allq = MAZI_FUNCTION:GetAllNpcQuest()
    for i,v in next,Allq do
        if i == Level then
            return v.NPCName
        end
    end
end
function MAZI_FUNCTION:GetAllNPC()
    for i,v in pairs(NPCList) do
        local Model = v.Model
        if Model:FindFirstChild("QuestFloor",true) then
            AllData.QuestNpc[Model.Name] = v
            AllData.QuestNpc[Model.Name].lowername = Model.Name:lower()
        end
    end
end
MAZI_FUNCTION:GetAllNPC()
function MAZI_FUNCTION:NearNPCLevel(Name)
    local key , min
    for i,v in pairs(NPCList) do
        local Model = v.Model
        local NPCCFrame
        if Model.Name == Name then
            if key and min then
                if (v.Model:FindFirstChild("QuestFloor",true).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < min then
                    key , min = i , (v.Model:FindFirstChild("QuestFloor",true).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                    NPCName = v.Model:FindFirstChild("QuestFloor",true).CFrame
                end
            else
                key , min = i , (v.Model:FindFirstChild("QuestFloor",true).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                NPCName = v.Model:FindFirstChild("QuestFloor",true).CFrame
            end
        end
    end
    if key and min then
        return NPCName
    else
        return "API ERROR"
    end
end

function MAZI_FUNCTION:GetQuest(Level)
    local Quest = require(game:GetService("ReplicatedStorage").Quests)
    local Level = Level or game:GetService("Players").LocalPlayer.Data.Level.Value
    if Level >= 275 and Level <= 299 then
        Level = 250
    end
    if game.PlaceId == 2753915549 and Level >= 700 then
        Level = 699
    elseif game.PlaceId == 4442272183 and Level >= 1500 then
        Level = 1499
    elseif game.PlaceId == 7449423635 and Level >= 2400 then
        Level = 2399
    end
    local DataQuest = {}
    local less = math.huge
    for i,va in next,Quest do
        for c=1 ,#va do
            v = va[c]
            if Level >= v.LevelReq and not v.MeetsRequirements then
                if v.Task[table.foreach(v.Task,tostring)] ~= 1 and Level - v.LevelReq < less then
                    less = Level - v.LevelReq
                    DataQuest.Quest = {
                        Name = tostring(i),
                        Value = c,
                        Mon = (table.foreach(v.Task,tostring)),
                        Level = v.LevelReq,
                        NPCName = MAZI_FUNCTION:GetInfoNPCQuest(v.LevelReq),
                        Index = #va
                    }
                    if c > 1 then
                        DataQuest.PastQuest = {
                            Name = tostring(i),
                            Value = c-1,
                            Mon = (table.foreach(va[c-1].Task,tostring)),
                            Level = va[c-1].LevelReq,
                            NPCName = MAZI_FUNCTION:GetInfoNPCQuest(va[c-1].LevelReq)
                        }
                    end
                end
            end
        end
    end
    return DataQuest
end
-- [[ Accept Quest ]] --
function IsQuest(bool)
    return game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == bool
end
Questinfo = MAZI_FUNCTION:GetQuest()
game:GetService("Players").LocalPlayer.Data:WaitForChild("Level").Changed:Connect(function(v)
    Questinfo = MAZI_FUNCTION:GetQuest()
end)
function AcceptQuest()
    local Q = MAZI_FUNCTION:GetQuest().Quest
    if Q.Mon == 'Shanda' then
        local TargetIL , err = MAZI_FUNCTION:INIS(CFrame.new(-7894.61768, 5547.1416, -380.291199))
        local INIS = MAZI_FUNCTION:INIS(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        if TargetIL == 'SkyArea2' and INIS ~= TargetIL then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.SkyArea2.PathwayHouse.EntrancePoint.Position)
            wait(1)
        elseif TargetIL == 'SkyArea2' and INIS == 'SkyArea2' then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - MAZI_FUNCTION:NearNPCLevel(MAZI_FUNCTION:GetInfoNPCQuest(475)).Position).magnitude <= 1000 then
                local waittime = MAZI_FUNCTION:Tween(MAZI_FUNCTION:NearNPCLevel(MAZI_FUNCTION:GetInfoNPCQuest(475)) * CFrame.new(0,5,0))
                wait(waittime)
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest",Q.Name,Q.Value)
            else
                MAZI_FUNCTION:Tween(MAZI_FUNCTION:NearNPCLevel(MAZI_FUNCTION:GetInfoNPCQuest(475)) * CFrame.new(0,5,0))
            end
        end
    else
        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - AllData.QuestNpc[Q.NPCName].Model:FindFirstChild("QuestFloor",true).CFrame.Position).magnitude <= 2000 then
            local waittime = MAZI_FUNCTION:TP(AllData.QuestNpc[Q.NPCName].Model:FindFirstChild("QuestFloor",true).CFrame * CFrame.new(0,5,0))
            wait(waittime)
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest",Q.Name,Q.Value)
        else
            MAZI_FUNCTION:TP(AllData.QuestNpc[Q.NPCName].Model:FindFirstChild("QuestFloor",true).CFrame)
        end
    end
end
-- [[ Auto Farm ]] --

function GetMobName()
    if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible then
        local Text = game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text:split(" ")
        local RealText = ""
        local List = #Text
        local Is = true
        if Text[List]:find("/1)") then
            Is = false
        end
        table.remove(Text,1)
        table.remove(Text,1)
        table.remove(Text,#Text)
        for i=1,#Text do local v = Text[i]
            RealText = RealText..v
            if #Text ~= i then
                RealText = RealText.." "
            end
        end
        if Is then
            RealText = RealText:sub(1,#RealText-1)
        end
        return RealText
    end
end
local DoubleQuestType = false
FarmFragments = false
function AutoFarmL()
    local HaveFruitOpen = false
    local HaveSaber = false
    local HaveFruitInStore = false
    while wait() do
        if SaveSettings["Farm"]['Auto Farm'] then
            pcall(function()
                local MyLevel = game:GetService("Players").LocalPlayer.Data.Level.Value
                local INIS = MAZI_FUNCTION:PlayerInIsland()
                for i,v in pairs(game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("getInventoryWeapons")) do
                    if v.Name == 'Saber' then
                        HaveSaber = true
                    end
                end
                if MyLevel >= 700 and game.PlaceId == 2753915549 then
                    if SaveSettings["Farm"]['Auto Saber'] and HaveSaber == false then
                        if game.Players.localPlayer.Data.Level.Value >= 200 then
                            if game.Workspace.Map.Jungle.Final.Part.CanCollide == false then
                                if game.Workspace.Enemies:FindFirstChild("Saber Expert [Lv. 200] [Boss]") then
                                    for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                                        if v.Name == "Saber Expert [Lv. 200] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                            repeat wait()
                                                Buso()
                                                NameMon = v.Name
                                                IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                                Positionmon = v.HumanoidRootPart.CFrame
                                                MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                                -- MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                                if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                                    usebringmob = true
                                                    UseFastattack = true
                                                else
                                                    UseFastattack = false
                                                    usebringmob = false
                                                end
                                            until not SaveSettings["Farm"]['Auto Saber'] or not v.Parent or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0
                                            UseFastattack = false
                                            IsnetworkOwnerMon = nil
                                            Positionmon = nil
                                        end
                                    end
                                else
                                    MAZI_FUNCTION:TP(CFrame.new(-1405.41956, 29.8519993, 5.62435055))
                                end
                            elseif game.Players.LocalPlayer.Backpack:FindFirstChild("Relic") or game.Players.LocalPlayer.Character:FindFirstChild("Relic") and game.Players.localPlayer.Data.Level.Value >= 200 then
