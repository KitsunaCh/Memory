function UpgradeStatfunction(stats,point)
    if game:GetService("Players").LocalPlayer.Data.Points.Value > 0 then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint",stats,point)
    end
end
coroutine.wrap(function()
    while wait() do
        PointValue:SetText('Point : ' .. LocalPlayer.Data.Points.Value)
        if game:GetService("Players").LocalPlayer.Data.Points.Value ~= 0 then
            if SaveSettings["Stats"]["Melee"] then
                UpgradeStatfunction('Melee',SaveSettings["Stats"]["Point"])
            end
            if SaveSettings["Stats"]["Defense"] then
                UpgradeStatfunction('Defense',SaveSettings["Stats"]["Point"])
            end
            if SaveSettings["Stats"]["Sword"] then 
                UpgradeStatfunction('Sword',SaveSettings["Stats"]["Point"])
            end
            if SaveSettings["Stats"]["Gun"] then 
                UpgradeStatfunction('Gun',SaveSettings["Stats"]["Point"])
            end
            if SaveSettings["Stats"]["Demon Fruit"] then
                UpgradeStatfunction("Demon Fruit",SaveSettings["Stats"]["Point"])
            end
        end
    end
end)()
