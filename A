    MainUICorner_2.Name = "MainUICorner"
    MainUICorner_2.Parent = Search

    TabSet.Name = "TabSet"
    TabSet.Parent = Search
    TabSet.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
    TabSet.Position = UDim2.new(0.5, -59, 0.5, -11)
    TabSet.Size = UDim2.new(0, 118, 0, 23)
    TabSet.Font = Enum.Font.SourceSansSemibold
    TabSet.PlaceholderText = "Search : ..."
    TabSet.Text = ""
    TabSet.TextColor3 = Color3.fromRGB(255, 255, 255)
    TabSet.TextSize = 18.000

    local Folder = Instance.new("Folder")

    Folder.Parent = Main

	TabSet:GetPropertyChangedSignal('Text'):Connect(function()
		local existframe = Folder:FindFirstChild("Main")
		if existframe then
			for i,v in pairs(existframe:GetChildren() ) do
                for i,v in pairs(v:GetChildren()) do
                    if v.Name == "MainSection" then
                        for i,v in pairs(v.SectionContent:GetChildren()) do
                            if v:FindFirstChild("Title") then
                                if v.Title.Text:lower():find(TabSet.Text:lower()) then
                                    v.Visible = true
                                else
                                    v.Visible = false
                                end
                            end
                        end
                    end
                end
			end
		end 
	end)

    TabSet.MouseEnter:Connect(function()
        TweenService:Create(Search,TweenInfo.new(.1),{BackgroundColor3 = Color3.fromRGB(135, 206, 235)}):Play()
    end)
    TabSet.MouseLeave:Connect(function()
        TweenService:Create(Search,TweenInfo.new(.1),{BackgroundColor3 = Color3.fromRGB(200, 200, 200)}):Play()
    end)

    MainUICorner_3.Name = "MainUICorner"
    MainUICorner_3.Parent = TabSet

