task.spawn(ESPFunction)

spawn(function()
    local HaveFruitInStore = false
    local function RemoveTextFruit(str) return str:gsub(" Fruit", "") end  
    while wait() do 
        if SaveSettings["Farm"]['Auto Buy Color Haki'] then
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ColorsDealer", "2")
        end
        if SaveSettings["Farm"]['Auto Buy Legendary Sword'] then
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("LegendarySwordDealer", "2")
        end
        if SaveSettings['Devil Fruit']["Auto Buy Devil Fruit Sinper"]  then
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("PurchaseRawFruit",SaveSettings['Devil Fruit']['Select Devil Fruit'],false)
        end   
        if SaveSettings['Devil Fruit']["Auto Random Devil Fruit"] then
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("Cousin", "Buy")
        end  
        if SaveSettings['Devil Fruit']["Bring Fruit"] then
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if v:IsA("Model") and string.find(v.Name,"Fruit") then
                    MAZI_FUNCTION:Tween(v.Handle.CFrame)
                end
            end
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if v:IsA("Tool") and string.find(v.Name,"Fruit") then wait()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
                    v.Handle.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                    -- firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,v.Handle,0)
                    game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):UnequipTools()
                end
            end
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if string.find(v.Name,"Fruit") then wait()
                    game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):UnequipTools()
                end
            end
        end 
        if SaveSettings['Devil Fruit']["Bring Fruit [ Hop ]"] then
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if v:IsA("Model") and string.find(v.Name,"Fruit") then
                    MAZI_FUNCTION:Tween(v.Handle.CFrame)
                end
            end
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if v:IsA("Tool") and string.find(v.Name,"Fruit") then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,v.Handle,0)
                    game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):UnequipTools()
                end
            end
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if string.find(v.Name,"Fruit") then
                    game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):UnequipTools()
                end
            end
            for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if string.find(v.Name,"Fruit") then
                    local FruitName = RemoveTextFruit(v.Name)
                    if v.Name == "Bird: Falcon Fruit" then
                        NameFruit = "Bird-Bird: Falcon"
                    elseif v.Name == "Bird: Phoenix Fruit" then
                        NameFruit = "Bird-Bird: Phoenix"
                    elseif v.Name == "Human: Buddha Fruit" then
                        NameFruit = "Human-Human: Buddha"
                    else
                        NameFruit = FruitName.."-"..FruitName
                    end

                    local string_1 = "getInventoryFruits";
                    local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                    for i1,v1 in pairs(Target:InvokeServer(string_1)) do
                        if v1.Name == NameFruit then
                            HaveFruitInStore = true
                        end
                    end
                    if not HaveFruitInStore then
                        local args = {
                            [1] = "StoreFruit",
                            [2] = NameFruit,
                            [3] = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v.Name)
                        }
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                    end
                    HaveFruitInStore = false
                end
            end
            if NoCLip or usenoclip or GotoTarget then else
                if SaveSettings['Devil Fruit']["Bring Fruit [ Hop ]"] then
                    task.delay(5, function()
                        game:GetService("StarterGui"):SetCore("SendNotification", {
                            Title = "HopServer";
                            Text = ". . ."
                        })
                        while wait() do
                            if SaveSettings['Devil Fruit']["Bring Fruit [ Hop ]"] then
                                HopServer()
                            end
                        end
                    end)
                end
            end
        end 
        if SaveSettings['Devil Fruit']["Auto Store Fruits"] then wait()
            for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if string.find(v.Name,"Fruit") then
                    local FruitName = RemoveTextFruit(v.Name)
                    if v.Name == "Bird: Falcon Fruit" then
                        NameFruit = "Bird-Bird: Falcon"
                    elseif v.Name == "Bird: Phoenix Fruit" then
                        NameFruit = "Bird-Bird: Phoenix"
                    elseif v.Name == "Human: Buddha Fruit" then
                        NameFruit = "Human-Human: Buddha"
                    else
                        NameFruit = FruitName.."-"..FruitName
                    end

                    local string_1 = "getInventoryFruits";
                    local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                    for i1,v1 in pairs(Target:InvokeServer(string_1)) do
                        if v1.Name == NameFruit then
                            HaveFruitInStore = true
                        end
                    end
                    if not HaveFruitInStore then
                        local args = {
                            [1] = "StoreFruit",
                            [2] = NameFruit,
                            [3] = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v.Name)
                        }
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                    end
                    HaveFruitInStore = false
                end
            end
        end
    end
end)

task.spawn(function()
    EliteSpawn = false
    while wait(1) do
        EliteText:SetText('Current Elite Hunter : ' .. (tostring(game.ReplicatedStorage.Remotes.CommF_:InvokeServer("EliteHunter", "Progress"))))
        for i,v in pairs(GetAllMob('all')) do
            if string.find(v.Name,"Diablo") or string.find(v.Name,"Urban") or string.find(v.Name,"Deandre") then
                EliteSpawnText:SetText('Elite Hunter : Spawn')
                EliteSpawn = true
            end
        end
        if EliteSpawn == false then
            EliteSpawnText:SetText('Elite Hunter : Not Spawn')
        end
        EliteSpawn = false
    end
end)
if workspace:WaitForChild("Map"):FindFirstChild("CircleIsland") then
    -- Button 1
    repeat wait()
        if game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Screen1.BrickColor == BrickColor.new("Really red") then else
            if game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Button1:FindFirstChild('ClickDetector') then
                fireclickdetector(game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Button1.ClickDetector)
            end
        end 
    until game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Screen1.BrickColor == BrickColor.new("Really red")
    -- Button 2
    repeat wait()
        if game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Screen2.BrickColor == BrickColor.new("Really blue") then else
            if game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Button2:FindFirstChild('ClickDetector') then
                fireclickdetector(game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Button2.ClickDetector)
            end
        end 
    until game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Screen2.BrickColor == BrickColor.new("Really blue")

    -- Button 3
    repeat wait()
        if game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Screen3.BrickColor == BrickColor.new("Lime green") then else
            if game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Button3:FindFirstChild('ClickDetector') then
                fireclickdetector(game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Button3.ClickDetector)
            end
        end 
    until game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Screen3.BrickColor == BrickColor.new("Lime green")
    -- Button 4
    repeat wait()
        if game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Screen4.BrickColor == BrickColor.new("Really blue") then else
            if game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Button4:FindFirstChild('ClickDetector') then
                fireclickdetector(game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Button4.ClickDetector)
            end
        end 
    until game:GetService("Workspace").Map.CircleIsland.Lab.Combo.Screen4.BrickColor == BrickColor.new("Really blue")
end

for i,v in next,game:GetService("Workspace").Map:GetDescendants() do
    if v.Name == 'Lava' then
        v:Destroy()
    end
end

game:GetService("Players").LocalPlayer.Idled:connect(function()
    game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    wait(1)
    game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function()
    NoCLip = false 
    usenoclip = false
    GotoTarget = false
end)
