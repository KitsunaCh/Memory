                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame.z)
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame.z)
                                
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame.z)
                                        
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame)
                                        end
                                    elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                                        game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame = CFrame.new(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame.x,150,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame.z)
                                        
                                        if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2000 then
                                            MAZI_FUNCTION:Tween(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame)
                                        end
                                    end
                                end
                                for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                                    if SaveSettings["Farm"]['Auto Superhuman'] and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and (v.HumanoidRootPart.Position-game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 1000 then
                                        pcall(function()
                                            repeat wait()
                                                isnetworkowner(v.HumanoidRootPart)
                                                v.Humanoid.Health = 0
                                            until not SaveSettings["Farm"]['Auto Superhuman'] or v.Humanoid.Health <= 0 or not v.Parent
                                        end)
                                    end
                                end
                            end
                        else
                            MAZI_FUNCTION:TP(MAZI_FUNCTION:GetRegions(Questinfo.Quest.Mon).CFrame)
                        end
                    end
                end
                UseFastattack = false
            end)
        end
    end
end
function AutoSuperhuman()
    while wait() do
        if SaveSettings["Farm"]['Auto Superhuman'] and SaveSettings["Farm"]['Auto Farm'] then 
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Combat") or game.Players.LocalPlayer.Character:FindFirstChild("Combat") then
                local args = {
                    [1] = "BuyElectro"
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end   
            if game.Players.LocalPlayer.Character:FindFirstChild("Superhuman") or game.Players.LocalPlayer.Backpack:FindFirstChild("Superhuman") then
                SaveSettings["Setting Auto Farm"]["Select Weapon"] = "Superhuman"
            end  
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Black Leg") and game.Players.LocalPlayer.Backpack:FindFirstChild("Black Leg").Level.Value <= 299  then
                SaveSettings["Setting Auto Farm"]["Select Weapon"] = "Black Leg"
            end
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Electro") and game.Players.LocalPlayer.Backpack:FindFirstChild("Electro").Level.Value <= 299  then
                SaveSettings["Setting Auto Farm"]["Select Weapon"] = "Electro"
            end
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Fishman Karate") and game.Players.LocalPlayer.Backpack:FindFirstChild("Fishman Karate").Level.Value <= 299  then
                SaveSettings["Setting Auto Farm"]["Select Weapon"] = "Fishman Karate"
            end
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Dragon Claw") and game.Players.LocalPlayer.Backpack:FindFirstChild("Dragon Claw").Level.Value <= 299  then
                SaveSettings["Setting Auto Farm"]["Select Weapon"] = "Dragon Claw"
            end
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Black Leg") and game.Players.LocalPlayer.Backpack:FindFirstChild("Black Leg").Level.Value >= 300  then
                local args = {
                    [1] = "BuyFishmanKarate"
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end
            if game.Players.LocalPlayer.Character:FindFirstChild("Black Leg") and game.Players.LocalPlayer.Character:FindFirstChild("Black Leg").Level.Value >= 300  then
                local args = {
                    [1] = "BuyFishmanKarate"
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Electro") and game.Players.LocalPlayer.Backpack:FindFirstChild("Electro").Level.Value >= 300  then
                local args = {
                    [1] = "BuyBlackLeg"
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end
            if game.Players.LocalPlayer.Character:FindFirstChild("Electro") and game.Players.LocalPlayer.Character:FindFirstChild("Electro").Level.Value >= 300  then
                local args = {
                    [1] = "BuyBlackLeg"
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Fishman Karate") and game.Players.LocalPlayer.Backpack:FindFirstChild("Fishman Karate").Level.Value >= 300 then
                if game.Players.LocalPlayer.Data.Fragments.Value < 1500 and game.ReplicatedStorage.Remotes.CommF_:InvokeServer("BlackbeardReward", "DragonClaw", "1") ~= 1 then wait(1)
                    if game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false then 
                        FarmFragments = true
                    elseif game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false and 
                        ( game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("RaidsNpc", "Select", SaveSettings["Raids"]['Select Raids']):find("physical fruit to ignore this cooldown.") 
                        or game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("RaidsNpc", "Select", SaveSettings["Raids"]['Select Raids']):find("buy another Special Microchip.") )
                    then
                        FarmFragments = false
                    else
                        FarmFragments = true
                    end
                else
                    local args = {
                        [1] = "BlackbeardReward",
                        [2] = "DragonClaw",
                        [3] = "2"
                    }
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                end
            end
            if game.Players.LocalPlayer.Character:FindFirstChild("Fishman Karate") and game.Players.LocalPlayer.Character:FindFirstChild("Fishman Karate").Level.Value >= 300  then
                if game.Players.LocalPlayer.Data.Fragments.Value < 1500 and game.ReplicatedStorage.Remotes.CommF_:InvokeServer("BlackbeardReward", "DragonClaw", "1") ~= 1 then wait(1)
                    if game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false then 
                        FarmFragments = true
                    elseif game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false and 
                        ( game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("RaidsNpc", "Select", SaveSettings["Raids"]['Select Raids']):find("physical fruit to ignore this cooldown.") 
                        or game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("RaidsNpc", "Select", SaveSettings["Raids"]['Select Raids']):find("buy another Special Microchip.") )
                    then
                        FarmFragments = false
                    else
                        FarmFragments = true
                    end
                else
                    local args = {
                        [1] = "BlackbeardReward",
                        [2] = "DragonClaw",
                        [3] = "2"
                    }
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                end
            end
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Dragon Claw") and game.Players.LocalPlayer.Backpack:FindFirstChild("Dragon Claw").Level.Value >= 300  then
                local args = {
                    [1] = "BuySuperhuman"
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end
            if game.Players.LocalPlayer.Character:FindFirstChild("Dragon Claw") and game.Players.LocalPlayer.Character:FindFirstChild("Dragon Claw").Level.Value >= 300  then
                local args = {
                    [1] = "BuySuperhuman"
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end 
        end
    end
end
-- Sea 2
function AutoFarmLawBoss()
    while wait() do
        if SaveSettings["Farm"]['Auto Farm Boss Law'] then
            if (CFrame.new(-6402, 49, -4958).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 300 then
                if game.Workspace.Enemies:FindFirstChild("Order [Lv. 1250] [Raid Boss]") then 
                    for i,v in pairs(GetAllMob()) do
                        if v.Name == 'Order [Lv. 1250] [Raid Boss]' then
                            repeat wait()
                                Buso()
                                if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                    MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,50,30))
                                    if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 100 then
                                        UseFastattack = true
                                    else
                                        UseFastattack = false
                                    end
                                end 
                            until not SaveSettings["Farm"]['Auto Farm Boss Law'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                            UseFastattack = false
                            IsnetworkOwnerMon = nil
                            Positionmon = nil
                        end
                    end
                else
                    if game.Players.LocalPlayer.Backpack:FindFirstChild('Microchip') or game.Players.LocalPlayer.Character:FindFirstChild('Microchip') then
                        fireclickdetector(game:GetService("Workspace").Map.CircleIsland.RaidSummon.Button.Main.ClickDetector)
                    else 
                        game.ReplicatedStorage.Remotes.CommF_:InvokeServer("BlackbeardReward", "Microchip", "2")
                    end
                end
            else
                MAZI_FUNCTION:TP(CFrame.new(-6402, 49, -4958))
            end
        end
    end
end
function AutoSwordRengoku()
    while wait() do
        if SaveSettings["Farm"]['Auto Rengoku'] then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFrame.new(5518.00684, 60.5559731, -6828.80518).Position).magnitude <= 500 or game.Players.LocalPlayer.Backpack:FindFirstChild("Hidden Key") or game.Players.LocalPlayer.Character:FindFirstChild("Hidden Key") then
                if game.Players.LocalPlayer.Backpack:FindFirstChild("Hidden Key") or game.Players.LocalPlayer.Character:FindFirstChild("Hidden Key") then
                    EquipWeapon("Hidden Key")
                    MAZI_FUNCTION:TP(CFrame.new(6571.81885, 296.689758, -6966.76514))
                elseif game.Workspace.Enemies:FindFirstChild("Snow Lurker [Lv. 1375]") then
                    for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if SaveSettings["Farm"]['Auto Rengoku'] and v.Name == "Snow Lurker [Lv. 1375]" and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                            repeat wait()
                                Buso()
                                NameMon = v.Name
                                IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                Positionmon = v.HumanoidRootPart.CFrame
                                MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,20,10))
                                if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 30 then
                                    usebringmob = true
                                    UseFastattack = true
                                else
                                    UseFastattack = false
                                    usebringmob = false
                                end
                            until not SaveSettings["Farm"]['Auto Rengoku'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or game.Players.LocalPlayer.Backpack:FindFirstChild("Hidden Key") or game.Players.LocalPlayer.Character:FindFirstChild("Hidden Key")
                            UseFastattack = false
                            IsnetworkOwnerMon = nil
                            Positionmon = nil
                        end
                    end
                else
                    MAZI_FUNCTION:TP(CFrame.new(5518.00684, 60.5559731, -6828.80518))
                end
            else
                MAZI_FUNCTION:TP(CFrame.new(5518.00684, 60.5559731, -6828.80518))
            end
        end
    end
end
-- Sea 3
function AutoFarmEliteHunter()
    while wait() do
        if SaveSettings["Farm"]['Auto Elite Hunter'] or SaveSettings["Farm"]['Auto Elite Hunter Hop'] then
            for i,v in pairs(GetAllMob('all')) do
                if IsQuest(true) then
                    if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Diablo") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Urban") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Deandre") then
                        if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Diablo") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Urban") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Deandre") then
                            if string.find(v.Name,"Diablo") or string.find(v.Name,"Urban") or string.find(v.Name,"Deandre") then
                                if SaveSettings["Farm"]['Auto Elite Hunter'] then
                                    repeat wait()
                                        Buso()
                                        if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                            IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                            MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                                UseFastattack = true
                                            else
                                                UseFastattack = false
                                            end
                                        end 
                                    until not SaveSettings["Farm"]['Auto Elite Hunter'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                    UseFastattack = false
                                    IsnetworkOwnerMon = nil
                                    Positionmon = nil
                                elseif SaveSettings["Farm"]['Auto Elite Hunter Hop'] then
                                    repeat wait()
                                        Buso()
                                        if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                            IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                            MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                                UseFastattack = true
                                            else
                                                UseFastattack = false
                                            end
                                        end 
                                    until not SaveSettings["Farm"]['Auto Elite Hunter Hop'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                    UseFastattack = false
                                    IsnetworkOwnerMon = nil
                                    Positionmon = nil
                                end
                            end
                        end
                    else 
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer('AbandonQuest')
                    end
                else
                    local string_1 = "EliteHunter";
                    local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
                    Target:InvokeServer(string_1);
                    if game:GetService('Players').LocalPlayer.Backpack:FindFirstChild("God's Chalice") or game:GetService('Players').LocalPlayer.Character:FindFirstChild("God's Chalice") or game:GetService('Players').LocalPlayer.Backpack:FindFirstChild("Sweet Chalice") or game:GetService('Players').LocalPlayer.Character:FindFirstChild("Sweet Chalice") then else
                        if SaveSettings["Farm"]['Auto Elite Hunter Hop'] and game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("EliteHunter") == "I don't have anything for you right now. Come back later." then
                            HopServer()
                        end
                    end
                end
            end
        end
    end
end
function AutoFarmTrainingDummy()
    while wait() do
        if SaveSettings["Farm"]['Auto Training Dummy'] then
            if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ArenaTrainer") == 'Oh, it appears that you already completed this quest recently. Remember, you can only do this quest once per hour!' then else
                if (CFrame.new(3666.44458, 91.4550705, 256.502441).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 150 then
                    game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ArenaTrainer")
                    for i,v in pairs(GetAllMob()) do
                        if v.Name == 'Training Dummy [Lv. 1500]' then
                            repeat wait()
                                Buso()
                                if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                    MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                    if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                        UseFastattack = true
                                    else
                                        UseFastattack = false
                                    end
                                end 
                            until not SaveSettings["Farm"]['Auto Training Dummy'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                            UseFastattack = false
                            IsnetworkOwnerMon = nil
                            Positionmon = nil
                        end
                    end
                else
                    MAZI_FUNCTION:TP(CFrame.new(3666.44458, 91.4550705, 256.502441))
                end
            end
        end
    end
end
function AutoFarmBuddySwords()
    while wait() do
        if SaveSettings["Farm"]['Auto Farm Buddy Swords'] or SaveSettings["Farm"]['Auto Farm Buddy Swords Hop'] then
            for i,v in pairs(GetAllMob('all')) do
                if SaveSettings["Farm"]['Auto Farm Buddy Swords Hop'] or SaveSettings["Farm"]['Auto Farm Buddy Swords'] then
                    if game.ReplicatedStorage:FindFirstChild("Cake Queen [Lv. 2175] [Boss]") or game.Workspace.Enemies:FindFirstChild("Cake Queen [Lv. 2175] [Boss]") then
                        if v.Name == "Cake Queen [Lv. 2175] [Boss]" then
                            if SaveSettings["Farm"]['Auto Farm Buddy Swords'] then
                                repeat wait()
                                    Buso()
                                    if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                        IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                        MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                            UseFastattack = true
                                        else
                                            UseFastattack = false
                                        end
                                    end 
                                until not SaveSettings["Farm"]['Auto Farm Buddy Swords'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                UseFastattack = false
                                IsnetworkOwnerMon = nil
                                Positionmon = nil
                            elseif SaveSettings["Farm"]['Auto Farm Buddy Swords Hop'] then
                                repeat wait()
                                    Buso()
                                    if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                        IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                        MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                            UseFastattack = true
                                        else
                                            UseFastattack = false
                                        end
                                    end 
                                until not SaveSettings["Farm"]['Auto Farm Buddy Swords Hop'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                UseFastattack = false
                                IsnetworkOwnerMon = nil
                                Positionmon = nil
                            end
                        end
                    else
                        if SaveSettings["Farm"]['Auto Farm Buddy Swords Hop'] then
                            HopServer()
                        end
                    end
                end
            end
        end
    end
end
function AutoHakiRainbow()
    while wait() do
        if SaveSettings["Farm"]['Auto Haki Rainbow'] then
            for i,v in pairs(GetAllMob('all')) do
                if game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true then
                    if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Stone") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Island Empress") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Kilo Admiral") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Captain Elephant") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Beautiful Pirate") then
                        if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Stone") then
                            if game.Workspace.Enemies:FindFirstChild("Stone [Lv. 1550] [Boss]") then
                                if v.Name == "Island Empress [Lv. 1675] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    repeat wait()
                                        Buso()
                                        IsnetworkOwnerMon = isnetworkowner(v.HumanoidRootPart)
                                        MAZI_FUNCTION:TP(v.HumanoidRootPart.CFrame * CFrame.new(0,15,10))
                                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 20 then
                                            UseFastattack = true
                                        else
                                            UseFastattack = false
                                        end
                                    until not SaveSettings["Farm"]['Auto Haki Rainbow'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                    UseFastattack = false
                                    IsnetworkOwnerMon = nil
                                    Positionmon = nil
                                end
                            else 
                                MAZI_FUNCTION:TP(CFrame.new(-1134, 40, 6877))
                            end
                        elseif string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Island Empress") then
                            if game.Workspace.Enemies:FindFirstChild("Island Empress [Lv. 1675] [Boss]") then
