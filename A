                                            UseFastattack = true
                                        else
                                            UseFastattack = false
                                        end
                                    until not SaveSettings["Farm"]['Auto Haki Rainbow'] or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                                    UseFastattack = false
                                    IsnetworkOwnerMon = nil
                                    Positionmon = nil
                                end
                            else
                                MAZI_FUNCTION:TP(CFrame.new(5614, 603, 339))
                            end	
                        elseif string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Kilo Admiral") then
                            if game.Workspace.Enemies:FindFirstChild("Kilo Admiral [Lv. 1750] [Boss]") then
                                if v.Name == "Kilo Admiral [Lv. 1750] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    repeat wait()
                                        Buso()
