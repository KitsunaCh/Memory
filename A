    function Library:ChangePresetColor(toch,mode)
        if mode == "Line" then
            PresetColorLine = toch
        end
        if mode == "Slider" then
            PresetColorSlider = toch
        end
        if mode == "Toggle" then
            PresetColorToggle = toch
        end
    end

    function Library:Tab(Name,Mode)
        local Tab = Instance.new("TextButton")
        local TabUICorner = Instance.new("UICorner")

        Tab.Name = "Tab"
        Tab.Parent = BalckGrouitList
        Tab.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        Tab.Position = UDim2.new(0, 0, 0.137704924, 0)
        Tab.Size = UDim2.new(0, 115, 0, 25)
        Tab.Font = Enum.Font.SourceSansSemibold
        Tab.Text = Name
        Tab.TextColor3 = Color3.fromRGB(150, 150, 150)
        Tab.TextSize = 20.000
        Tab.TextWrapped = true
        
        TabUICorner.Name = "TabUICorner"
        TabUICorner.Parent = Tab

        local Section = Instance.new("Frame")

        Section.Name = "Section"
        Section.Parent = MainUi
        Section.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Section.BackgroundTransparency = 1.000
        Section.Position = UDim2.new(0.200000003, 0, 0.115, 0)
        Section.Size = UDim2.new(0, 520, 0, 340)
        Section.Visible = false

        if FS == nil then
            FS = true
            Section.Visible = true
            Tab.TextColor3 = Color3.fromRGB(135, 206, 235)
        end

        Tab.MouseButton1Click:Connect(function()
            for i,v in pairs(Folder.Main:GetChildren()) do
                v.Visible = false
                for i,v in pairs(v:GetChildren()) do
                    if v.Name == "MainSection" then
                        if Mode == "One" then
                            v.Size = UDim2.new(0, 503, 0, 0)
                        elseif Mode == "Two" then
                            v.Size = UDim2.new(0, 251, 0, 0)
                        end 
                    end
                end
            end
            for i,v in pairs(BalckGrouitList:GetChildren()) do
                if v:IsA("TextButton") then
                    TweenService:Create(v, TweenInfo.new(1, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextColor3 = Color3.fromRGB(150, 150, 150)}):Play()
                end
            end
            for i,v in pairs(Folder.Main:GetChildren()) do
                Section.Visible = true
                for i,v in pairs(v:GetChildren()) do
                    if v.Name == "MainSection" then
                        if Mode == "One" then
                            TweenService:Create(v, TweenInfo.new(1, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Size = UDim2.new(0, 503, 0, 323)}):Play()
                        elseif Mode == "Two" then
                            TweenService:Create(v, TweenInfo.new(1, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Size = UDim2.new(0, 251, 0, 323)}):Play()
                        end 
                    end
                end
            end
            TweenService:Create(Tab, TweenInfo.new(1, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextColor3 = Color3.fromRGB(135, 206, 235)}):Play()
        end)

        SectionElements = {}
        if Mode == "One" then
            local UIListLayout = Instance.new("UIListLayout")
            UIListLayout.Parent = Section
            UIListLayout.FillDirection = Enum.FillDirection.Horizontal
            UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            -- UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
            UIListLayout.Padding = UDim.new(0, 10)

            function SectionElements:Section(Name)
                local MainSection = Instance.new("ImageLabel")
                local SectionBorder = Instance.new("ImageLabel")
                local SectionTitle = Instance.new("TextLabel")
                local SectionContent = Instance.new("ScrollingFrame")
                local SectionContentLayout = Instance.new("UIListLayout")
                
                MainSection.Name = "MainSection"
                MainSection.Parent = Section
                MainSection.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                MainSection.BackgroundTransparency = 1.000
                MainSection.Position = UDim2.new(0.0266666673, 0, 0.0343861282, 0)
                MainSection.Size = UDim2.new(0, 503, 0, 323)
                MainSection.ZIndex = 4
                MainSection.Image = "rbxassetid://3570695787"
                MainSection.ImageColor3 = Color3.fromRGB(40, 40, 40)
                MainSection.ScaleType = Enum.ScaleType.Slice
                MainSection.SliceCenter = Rect.new(100, 100, 100, 100)
                MainSection.SliceScale = 0.050

                SectionBorder.Name = "SectionBorder"
                SectionBorder.Parent = MainSection
