        elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 2 then
            MoonStatsPhase:SetText('Moon Stats : ')
        elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 3 then
            MoonStatsPhase:SetText('Moon Stats : ')
        elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 4 then
            MoonStatsPhase:SetText('Moon Stats : ')
        elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 5 then
            MoonStatsPhase:SetText('Moon Stats : ')
        elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 6 then
            MoonStatsPhase:SetText('Moon Stats : ')
        elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 7 then
            MoonStatsPhase:SetText('Moon Stats : ')
        elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 8 then
            MoonStatsPhase:SetText('Moon Stats : ')
        elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 9 then
            MoonStatsPhase:SetText('Moon Stats : ')
        else
            MoonStatsPhase:SetText('Moon Stats : ')
        end
    end
end)
