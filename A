                                if input.UserInputType == Enum.UserInputType.MouseMovement then
                                    TweenService:Create(NameButton, TweenInfo.new(0.35, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.95}):Play()
