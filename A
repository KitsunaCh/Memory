                                    wait(.1)
                                end
                                TweenService:Create(lock, TweenInfo.new(0.1, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Rotation = 0}):Play()
                            else
                                Toggled = not Toggled
                                SetState(Toggled)
                                table_function.callback(Toggled)
                            end
                        end)

