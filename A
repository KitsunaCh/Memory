                            function()
                                while wait() do
                                    UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, PresetColorToggle), ColorSequenceKeypoint.new(1.00, PresetColorToggle)}
                                    UIGradient_2.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, PresetColorToggle), ColorSequenceKeypoint.new(1.00, PresetColorToggle)}
                                end
                            end
