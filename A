local NPCList = getupvalues(require(game:GetService("ReplicatedStorage").Queue).new)[1][1].NPCDialogueEnabler.bin
local SpawnList = game:GetService("Workspace")["_WorldOrigin"].PlayerSpawns.Pirates:GetChildren()
-- [[ Function ]] --
function Buso()
    if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") and SaveSettings["Setting Auto Farm"]['Fast Attack'] then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
    end
    EquipWeapon(SaveSettings["Setting Auto Farm"]["Select Weapon"])
end
function EquipWeapon(ToolSe)
    if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
        local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
        wait(.4)
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
    end
end
-- [[ Island & Teleport ]] --
local MAP = game:GetService("Workspace").Map:GetChildren()
local Team = game:GetService("Players").LocalPlayer.Team
local MAZI_FUNCTION = {}
local MAPONFO = {}
for i,v in next , MAP do
    if v:IsA("Model") then
        MAPONFO[v] = {
            Name = v.Name,
            CFrame = v:GetModelCFrame(),
            Size = v:GetExtentsSize(),
            Position = v:GetModelCFrame().Position
        }
    end
end
function MAZI_FUNCTION:Tween(CFrame)
    Distance = (CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
    if Distance < 100 then
        Speed = 200
    elseif Distance < 300 then
        Speed = 400
    elseif Distance < 1000 then
        Speed = 350
    elseif Distance >= 1000 then
        Speed = 250
    end
    if tween then
        tween:Cancel()
        usenoclip = false
    end
    if Distance < 150 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame
    else
        tween = game:GetService("TweenService"):Create(
            game.Players.LocalPlayer.Character.HumanoidRootPart,
            TweenInfo.new(Distance / Speed, Enum.EasingStyle.Linear),
            {CFrame = CFrame}
        )
        tween:Play()
        usenoclip = true
        tween.Completed:Connect(function(PlaybackState)
            usenoclip = false
        end)
    end
    return Distance / Speed
end
function MAZI_FUNCTION:INIS(Target)
    local DATA = {}
    local key , min
    for i,v in next,MAPONFO do 
        if key and min then
            if (v.CFrame.Position - Target.Position).magnitude < min then
                key , min = i , (v.CFrame.Position - Target.Position).magnitude
            end
        else
            key , min = i , (v.CFrame.Position - Target.Position).magnitude
        end
    end
    if key and min then
        if min <= MAPONFO[key].Position.magnitude then
            return MAPONFO[key].Name
        else
            return "Not Found IsLand!!" , MAPONFO[key].Name
        end
    else
        return "API ERROR"
    end
end
function MAZI_FUNCTION:CheckInIsland(Target)
    local TargetIL , err = MAZI_FUNCTION:INIS(Target)
    local INIS = MAZI_FUNCTION:INIS(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
    return INIS == TargetIL
end
function MAZI_FUNCTION:PlayerInIsland()
    local INIS = MAZI_FUNCTION:INIS(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
    return INIS
end
function MAZI_FUNCTION:GetRegions(MonName)
    local key , min = 0 , math.huge
    local Mon = 'None'
    MonName = MonName
    for i,v in next,EnemySpawns do
        if string.find(v.Name:lower(),MonName:lower()) or MonName:lower():sub(1,#v.Name) == v.Name then
            MonName = v.Name
        end
    end
    for i,EnemySpawns in next,game:GetService("Workspace")["_WorldOrigin"].EnemySpawns:GetChildren() do
        if string.find(EnemySpawns.Name,MonName) or EnemySpawns.Name == MonName then
            for i,v in next,game:GetService("Workspace")["_WorldOrigin"].EnemyRegions:GetChildren() do
                if (v.Position - EnemySpawns.Position).Magnitude < min then
                    key , min = i , (v.Position - EnemySpawns.Position).Magnitude
                    Mon = v
                end
            end
        end
    end
    if Mon == 'None' then
        for i,EnemySpawns in next,game:GetService("ReplicatedStorage"):GetChildren() do
            if string.find(EnemySpawns.Name,MonName) or EnemySpawns.Name == MonName then
                for i,v in next,game:GetService("Workspace")["_WorldOrigin"].EnemyRegions:GetChildren() do
                    if (v.Position - EnemySpawns.HumanoidRootPart.Position).Magnitude < min then
                        key , min = i , (v.Position - EnemySpawns.HumanoidRootPart.Position).Magnitude
                        Mon = v
                    end
                end
            end
        end
        for i,EnemySpawns in next,game:GetService("Workspace").Enemies:GetChildren() do
            if string.find(EnemySpawns.Name,MonName) or EnemySpawns.Name == MonName then
                for i,v in next,game:GetService("Workspace")["_WorldOrigin"].EnemyRegions:GetChildren() do
                    if (v.Position - EnemySpawns.HumanoidRootPart.Position).Magnitude < min then
                        key , min = i , (v.Position - EnemySpawns.HumanoidRootPart.Position).Magnitude
                        Mon = v
                    end
                end
            end
        end
    end
    return Mon
end
function MAZI_FUNCTION:TP(Target)
    if game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
        local DATA = {}
        local TargetIL , err = MAZI_FUNCTION:INIS(Target)
        local INIS = MAZI_FUNCTION:INIS(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        local GetUnlockables = game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer('GetUnlockables')
        warn(TargetIL,INIS)
        if TargetIL == 'Turtle' and game.PlaceId == 7449423635 and INIS ~= TargetIL then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",CFrame.new(-12551, 337, -7507).Position)
        elseif TargetIL == 'Boat Castle' and INIS ~= TargetIL then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",CFrame.new(-4999, 315, -3029).Position)
        elseif TargetIL == 'Waterfall' and INIS ~= TargetIL then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",CFrame.new(5729, 668, -195).Position)
        elseif TargetIL == 'Fishmen' and game.PlaceId == 2753915549 and INIS ~= TargetIL then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.TeleportSpawn.EntrancePoint.Position)
            wait(1)
        elseif TargetIL ~= 'Fishmen' and game.PlaceId == 2753915549 and INIS == 'Fishmen' then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.TeleportSpawn.ExitPoint.Position)
            wait(1)
        elseif TargetIL == 'SkyArea1' and game.PlaceId == 2753915549 and INIS ~= TargetIL then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.SkyArea1.PathwayTemple.ExitPoint.Position)
            wait(1)
        elseif TargetIL ~= 'SkyArea2' and game.PlaceId == 2753915549 and INIS == 'SkyArea2' then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.SkyArea1.PathwayTemple.ExitPoint.Position)
            wait(1)
        elseif TargetIL == 'SkyArea2' and game.PlaceId == 2753915549 and INIS ~= TargetIL then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.SkyArea2.PathwayHouse.EntrancePoint.Position)
            wait(1)
        elseif TargetIL == 'GraveIsland' and INIS ~= TargetIL and INIS ~= 'GhostShip' and game.PlaceId == 4442272183 then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.GhostShip.TeleportSpawn.Position)
            wait(1)
        elseif TargetIL ~= 'GhostShipInterior' and INIS == 'GhostShipInterior' and game.PlaceId == 4442272183 then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.GhostShip.TeleportSpawn.Position)
            wait(1)
        elseif TargetIL == 'GhostShipInterior' and INIS ~= TargetIL and game.PlaceId == 4442272183 then
            if tween then
                tween:Cancel()
                usenoclip = false
            end
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.GhostShipInterior.TeleportSpawn.Position)
            wait(1)
        end
        if TargetIL == "Not Found IsLand!!" or TargetIL == "API ERROR" then
            local TwDelay = MAZI_FUNCTION:Tween(Target)
            return warn(TargetIL)
        elseif INIS == TargetIL then
            local TwDelay = MAZI_FUNCTION:Tween(Target)
            return warn(TargetIL)
        else
            local TwDelay = MAZI_FUNCTION:Tween(Target)
            return warn(TargetIL)
        end
    end
end

-- [[ Island Teleprot ]]
for i,v in next,AllData['DataIslandName'][tostring(game.PlaceId)] do
    IslandTab:items('btn',{
        text = v,
        callback = function()
            MAZI_FUNCTION:TP(AllData['DataIsland'][v])
        end
    })
end
