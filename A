-- [[ function Shop ]] -- 
--[[
local TABA = Library:Tab("Material","One")
MaterialTab1 = TABA:Section('Material')
task.spawn(function()
    local ColorData = {
        [0] = { 
            "Common", "rgb(179, 179, 179)" 
        },
        { 
            "Uncommon", "rgb(92, 140, 211)" 
        }, 
        { 
            "Rare", "rgb(140, 82, 255)" 
        }, 
        { 
            "Legendary", "rgb(213, 43, 228)" 
        }, 
        { 
            "Mythical", "rgb(238, 47, 50)" 
        }, 
        { 
            "Premium", "rgb(221, 188, 0)" 
        }
    };
    local getInventory = game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer('getInventory');
    for index = 20,0,-1 do
        for i, Value in pairs(getInventory) do
            if Value.Type == 'Material' and index == Value.Rarity then
                do
                    spawn(function()
                        local Ass = MaterialTab1:items('Label',{text = '<font color="'.. ColorData[Value.Rarity][2] .. '">' .. Value.Name .. " : " .. tostring(Value.Count) .. "</font>"})
                        local Name = Value.Name
                        while wait(5) do
                            local getInventory = game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer('getInventory');
                            for i, ValueU in pairs(getInventory) do
                                if ValueU.Type == 'Material' and index == ValueU.Rarity and Name == ValueU.Name then
                                    if Value.Count == Value.MaxCount then
                                        Ass:SetText('<font color="'.. ColorData[ValueU.Rarity][2] .. '">' .. ValueU.Name .. " : " .. tostring(ValueU.Count) .. " ( Max )</font>")
                                    else
                                        Ass:SetText('<font color="'.. ColorData[ValueU.Rarity][2] .. '">' .. ValueU.Name .. " : " .. tostring(ValueU.Count) .. "</font>")
                                    end
                                end
                            end
                        end
                    end)
                end
            end
        end;
    end
end)
]]
